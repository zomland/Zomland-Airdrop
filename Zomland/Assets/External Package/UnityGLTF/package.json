{
  "name": "org.khronos.unitygltf",
  "displayName": "UnityGLTF",
  "version": "1.5.0-pre",
  "unity": "2018.4",
  "description": "Unity3D library for importing and exporting GLTF 2.0 assets. https://github.com/KhronosGroup/UnityGLTF",
  "keywords": [
    "gltf",
    "khronos",
    "runtime",
    "loader"
  ],
  "author": "Khronos Group",
  "samples": [
    {
      "displayName": "Sample Scenes",
      "description": "Various scenes showing GLTF functionality.",
      "path": "Samples~/Sample Scenes"
    },
    {
      "displayName": "Material Export Extensions",
      "description": "Example for custom export extension callbacks",
      "path": "Samples~/MaterialExportExtensions"
    },
    {
      "displayName": "Export TMPro materials as textures",
      "description": "Example for custom export callbacks.",
      "path": "Samples~/TMProAsTextureExport"
    }
  ],
  "dependencies": {
    "com.unity.nuget.newtonsoft-json": "2.0.0",
    "com.unity.modules.imageconversion": "1.0.0"
  }
}
